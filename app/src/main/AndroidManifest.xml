<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prianugrahw.weather">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_cloud"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_cloud"
        android:supportsRtl="true"
        android:theme="@style/Theme.Weather">
        <activity android:name=".AirQualityActivity"></activity>
        <activity android:name=".ForecastActivity" />
        <activity android:name=".DetailForecastActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>